{
    "name": "@flywave/flywave-transfer-manager",
    "version": "0.0.1",
    "description": "Provides a manager that orchestrates downloading URLs, particularly static map resources.",
    "main": "lib/src/index.js",
    "typings": "lib/src/index",
    "directories": {
        "test": "test",
        "lib": "src"
    },
    "exports": {
        ".": {
            "types": "./lib/src/index.d.ts",
            "default": "./lib/src/index.js"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "test": "cross-env mocha  --require source-map-support/register $EXTRA_MOCHA_ARGS ./test/*.js",
        "build": "cross-env tsc --build $EXTRA_TSC_ARGS",
        "prepare": "cross-env tsc --build $EXTRA_TSC_ARGS --force"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/flywave/flywave.gl.git",
        "directory": "@flywave/flywave-transfer-manager"
    },
    "author": {
        "name": "flywave.gl contributors",
        "url": "https://flywave.net"
    },
    "license": "Apache-2.0",
    "dependencies": {},
    "devDependencies": {
        "@types/chai": "catalog:",
        "@types/chai-as-promised": "catalog:",
        "@types/mocha": "catalog:",
        "@types/sinon": "catalog:",
        "chai": "catalog:",
        "chai-as-promised": "catalog:",
        "cross-env": "catalog:",
        "glob": "catalog:",
        "mocha": "catalog:",
        "sinon": "catalog:",
        "source-map-support": "catalog:",
        "typescript": "catalog:"
    },
    "private": true
}
