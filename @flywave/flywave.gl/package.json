{
    "name": "@flywave/flywave.gl",
    "version": "0.0.3",
    "type": "module",
    "description": "WebGl GIS",
    "exports": {
        ".": {
            "import": "./dist/flywave.gl.module.js",
            "require": "./dist/flywave.gl.cjs.js",
            "types": "./index.d.ts"
        },
        "./package.json": "./package.json",
        "./decoder": {
            "import": "./dist/flywave-decoders.js",
            "require": "./dist/flywave-decoders.js"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/index.d.ts"
            ]
        }
    },
    "files": [
        "dist/**/*",
        "README.md"
    ],
    "scripts": {
        "tsc": "tsc --build",
        "build": "webpack --mode production",
        "prepare": "webpack --mode production",
        "build-typings": "tsc --build && api-extractor run --local && echo 'export as namespace flywave;' >> dist/flywave.d.ts && rm -rf temp",
        "debug": "webpack"
    },
    "keywords": [
        "3d",
        "webgl",
        "gis",
        "map",
        "geospatial"
    ],
    "author": "flywave",
    "license": "Apache-2.0",
    "homepage": "https://github.com/flywave/flywave.gl#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/flywave/flywave.gl.git"
    },
    "bugs": {
        "url": "https://github.com/flywave/flywave.gl/issues"
    },
    "peerDependencies": {
        "three": ">= 0.169.0 < 0.182.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "gitHead": "d181da944fa31240e3c511aae6ccf2cfbdc252a7"
}
