{
    "name": "@flywave/flywave.gl",
    "version": "0.0.1",
    "type": "module",
    "description": "WebGl GIS",
    "exports": {
        ".": {
            "import": "./dist/flywave.gl.module.js",
            "require": "./dist/flywave.gl.cjs.js",
            "types": "./index.d.ts"
        },
        "./package.json": "./package.json",
        "./decoder": {
            "import": "./dist/flywave-decoders.js",
            "require": "./dist/flywave-decoders.js"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/index.d.ts"
            ]
        }
    },
    "files": [
        "dist/**/*",
        "README.md"
    ],
    "scripts": {
        "tsc": "tsc --build",
        "build": "webpack --mode production",
        "prepare": "webpack --mode production",
        "build-typings": "tsc --build && api-extractor run --local && echo 'export as namespace flywave;' >> dist/flywave.d.ts && rm -rf temp",
        "debug": "webpack"
    },
    "keywords": [
        "3d",
        "webgl",
        "gis",
        "map",
        "geospatial"
    ],
    "author": "flywave",
    "license": "Apache-2.0",
    "homepage": "https://github.com/flywave/flywave.gl#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/flywave/flywave.gl.git"
    },
    "bugs": {
        "url": "https://github.com/flywave/flywave.gl/issues"
    },
    "peerDependencies": {
        "three": ">= 0.169.0 < 0.182.0"
    },
    "devDependencies": {
        "@flywave/flywave-3dtile-datasource": "0.0.1",
        "@flywave/flywave-datasource-protocol": "0.0.1",
        "@flywave/flywave-draw-controls": "0.0.1",
        "@flywave/flywave-features-datasource": "0.0.1",
        "@flywave/flywave-geoutils": "0.0.1",
        "@flywave/flywave-inspector": "0.0.1",
        "@flywave/flywave-map-controls": "0.0.1",
        "@flywave/flywave-mapview": "0.0.1",
        "@flywave/flywave-mapview-decoder": "0.0.1",
        "@flywave/flywave-terrain-datasource": "0.0.1",
        "@flywave/flywave-utils": "0.0.1",
        "@flywave/flywave-vectortile-datasource": "0.0.1",
        "@flywave/flywave-webpack-utils": "0.0.1",
        "@flywave/flywave-webtile-datasource": "0.0.1",
        "copy-webpack-plugin": "catalog:",
        "cross-env": "catalog:",
        "expose-loader": "5.0.1",
        "tsconfig-paths-webpack-plugin": "catalog:",
        "typescript": "catalog:",
        "webpack": "catalog:",
        "webpack-cli": "catalog:",
        "webpack-merge": "catalog:"
    },
    "publishConfig": {
        "access": "public"
    }
}