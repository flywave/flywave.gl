{
    "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
    "mainEntryPointFilePath": "<projectFolder>/lib/index.d.ts",
    "bundledPackages": [
        "@flywave/flywave-mapview",
        "@flywave/flywave-webtile-datasource",
        "@flywave/flywave-features-datasource",
        "@flywave/flywave-geojson-datasource",
        "@flywave/flywave-terrain-datasource",
        "@flywave/flywave-vectortile-datasource",
        "@flywave/flywave-3dtile-datasource",
        "@flywave/flywave-draw-controls",
        "@flywave/flywave-map-controls",
        "@flywave/flywave-inspector"
    ],
    "compiler": {
        "tsconfigFilePath": "<projectFolder>/tsconfig.json",
        "overrideTsconfig": {
            "include": ["lib/**/*.d.ts"],
            "exclude": [
                "src/**/*",
                "node_modules/**/*",
                "dist/**/*",
                "**/*.test.ts",
                "**/*.spec.ts",
                "**/test/**/*"
            ],
            "compilerOptions": {
                "skipLibCheck": true,
                "noImplicitAny": false,
                "resolveJsonModule": true,
                "esModuleInterop": true,
                "paths": {
                    "@flywave/flywave-mapview": ["../flywave-mapview/lib/src"],
                    "@flywave/flywave-webtile-datasource": ["../flywave-webtile-datasource/lib/src"],
                    "@flywave/flywave-features-datasource": ["../flywave-features-datasource/lib/src"],
                    "@flywave/flywave-geojson-datasource": ["../flywave-geojson-datasource/lib/src"],
                    "@flywave/flywave-terrain-datasource": ["../flywave-terrain-datasource/lib/src"],
                    "@flywave/flywave-vectortile-datasource": ["../flywave-vectortile-datasource/lib/src"],
                    "@flywave/flywave-3dtile-datasource": ["../flywave-3dtile-datasource/lib/src"],
                    "@flywave/flywave-draw-controls": ["../flywave-draw-controls/lib/src"],
                    "@flywave/flywave-map-controls": ["../flywave-map-controls/lib/src"],
                    "@flywave/flywave-inspector": ["../flywave-inspector/lib/src"]
                }
            }
        }
    },
    "apiReport": {
        "enabled": false,
        "reportFileName": "<unscopedPackageName>.api.md"
    },
    "docModel": {
        "enabled": true,
        "apiJsonFilePath": "<projectFolder>/temp/<unscopedPackageName>.api.json"
    },
    "dtsRollup": {
        "enabled": true,
        "untrimmedFilePath": "dist/flywave.d.ts",
        "publicTrimmedFilePath": "",
        "omitTrimmingComments": true
    },
    "tsdocMetadata": {
        "enabled": false
    },
    "newlineKind": "lf",
    "messages": {
        "compilerMessageReporting": {
            "default": {
                "logLevel": "none"
            }
        },
        "extractorMessageReporting": {
            "default": {
                "logLevel": "none",
                "addToApiReportFile": false
            },
            "ae-missing-release-tag": {
                "logLevel": "none",
                "addToApiReportFile": false
            },
            "ae-forgotten-export": {
                "logLevel": "none",
                "addToApiReportFile": false
            },
            "ae-internal-missing-underscore": {
                "logLevel": "none",
                "addToApiReportFile": false
            },
            "ae-incompatible-release-tags": {
                "logLevel": "none",
                "addToApiReportFile": false
            }
        },
        "tsdocMessageReporting": {
            "default": {
                "logLevel": "none",
                "addToApiReportFile": false
            }
        }
    }
}
