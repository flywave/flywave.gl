{
    "name": "@flywave/flywave-topo",
    "version": "0.0.1",
    "description": "Topological data processing module for flywave.gl",
    "main": "lib/src/index.js",
    "typings": "lib/src/index",
    "exports": {
        ".": {
            "import": "./lib/src/index.js",
            "require": "./lib/src/index.js"
        },
        "./*": {
            "import": "./lib/src/*",
            "require": "./lib/src/*"
        },
        "./package.json": "./package.json"
    },
    "directories": {
        "test": "test"
    },
    "scripts": {
        "build": "cross-env tsc --build $EXTRA_TSC_ARGS",
        "test": "cross-env mocha --require source-map-support/register $EXTRA_MOCHA_ARGS ./test/*.js",
        "prepare": "cross-env tsc --build $EXTRA_TSC_ARGS --force"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/flywave/flywave.gl.git",
        "directory": "@flywave/flywave-topo"
    },
    "author": {
        "name": "flywave.gl contributors",
        "url": "https://flywave.net"
    },
    "license": "Apache-2.0",
    "dependencies": {
        "flatbuffers": "~1.12.0",
        "js-base64": "^3.7.5"
    },
    "devDependencies": {
        "@flywave/flywave-test-utils": "workspace:*",
        "@types/flatbuffers": "~1.10.0",
        "@types/chai": "catalog:",
        "@types/earcut": "catalog:",
        "@types/long": "catalog:",
        "@types/mocha": "catalog:",
        "@types/node": "catalog:",
        "@types/sinon": "catalog:",
        "@types/three": "catalog:",
        "chai": "catalog:",
        "copyfiles": "catalog:",
        "cross-env": "catalog:",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "mocha": "catalog:",
        "sinon": "catalog:",
        "source-map-support": "catalog:",
        "typescript": "catalog:"
    },
    "peerDependencies": {
        "three": "catalog:"
    },
    "private": true
}