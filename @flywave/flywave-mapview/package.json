{
    "name": "@flywave/flywave-mapview",
    "version": "0.0.1",
    "description": "Functionality needed to render a map.",
    "keywords": [
        "map",
        "map-renderer",
        "vector-tiles",
        "geojson",
        "typescript",
        "webgl",
        "flywave"
    ],
    "main": "lib/src/index.js",
    "types": "lib/src/index",
    "exports": {
        ".": {
            "import": "./lib/src/index.js",
            "require": "./lib/src/index.js",
            "types": "./lib/src/index.d.ts"
        },
        "./*": {
            "import": "./lib/src/*",
            "require": "./lib/src/*",
            "types": "./lib/src/*"
        },
        "./package.json": "./package.json"
    },
    "directories": {
        "test": "test"
    },
    "scripts": {
        "build": "cross-env tsc --build $EXTRA_TSC_ARGS",
        "test": "cross-env mocha --require source-map-support/register $EXTRA_MOCHA_ARGS ./test/*.js",
        "prepare": "cross-env tsc --build $EXTRA_TSC_ARGS --force"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/flywave/flywave.gl.git",
        "directory": "@flywave/flywave-mapview"
    },
    "author": {
        "name": "flywave.gl contributors",
        "url": "https://flywave.net"
    },
    "license": "Apache-2.0",
    "dependencies": {
        "rbush": "catalog:",
        "postprocessing": "catalog:"
    },
    "devDependencies": {
        "@flywave/flywave-test-utils": "workspace:*",
        "@types/chai": "catalog:",
        "@types/chai-as-promised": "catalog:",
        "@types/mocha": "catalog:",
        "@types/node": "catalog:",
        "@types/sinon": "catalog:",
        "@types/three": "catalog:",
        "chai": "catalog:",
        "chai-as-promised": "catalog:",
        "copyfiles": "catalog:",
        "cross-env": "catalog:",
        "mocha": "catalog:",
        "sinon": "catalog:",
        "source-map-support": "catalog:",
        "three": "catalog:",
        "typescript": "catalog:"
    },
    "peerDependencies": {
        "three": "catalog:"
    },
    "private": true
}
